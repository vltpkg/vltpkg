{
  "name": "@vltpkg/vltpkg",
  "version": "0.0.0-0",
  "private": true,
  "dependencies": {
    "vlt": "workspace:*"
  },
  "devDependencies": {
    "@eslint/js": "catalog:",
    "@types/eslint__js": "catalog:",
    "@vltpkg/benchmark": "workspace:*",
    "chalk": "catalog:",
    "eslint": "catalog:",
    "globals": "^15.9.0",
    "prettier": "catalog:",
    "semver": "catalog:",
    "turbo": "^2.1.0",
    "typedoc": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:",
    "yaml": "^2.5.0"
  },
  "license": "BSD-2-Clause-Patent",
  "engines": {
    "node": "20 || >=22"
  },
  "scripts": {
    ".": "./scripts/local-cli vlt",
    "prebenchmark": "pnpm vlt-benchmark-download-fixtures",
    "benchmark": "pnpm -r --sequential --no-bail --if-present run \"/^benchmark*/\"",
    "fix": "pnpm fix:pkg && pnpm lint && pnpm format",
    "fix:pkg": "node scripts/consistent-package-json.js",
    "format": "prettier --write . --log-level warn --ignore-path ./.prettierignore --cache",
    "format:check": "prettier --check . --ignore-path ./.prettierignore --cache",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "snap": "pnpm --silent --no-bail --report-summary run -r snap -Rsilent || node scripts/report-test-failures.js",
    "test": "pnpm --silent --no-bail --report-summary -r test -- -Rsilent || node scripts/report-test-failures.js",
    "typedoc": "typedoc",
    "vlix": "./scripts/local-cli vlix",
    "vlr": "./scripts/local-cli vlr",
    "vlrx": "./scripts/local-cli vlrx",
    "vlt": "./scripts/local-cli vlt",
    "vlx": "./scripts/local-cli vlx"
  },
  "prettier": "./.prettierrc.json",
  "type": "module"
}
