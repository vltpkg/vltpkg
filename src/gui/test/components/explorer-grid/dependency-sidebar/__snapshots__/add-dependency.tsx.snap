exports[`add-dependencies-popover render default 1`] = `

<gui-card-header classname="relative flex w-full flex-col rounded-t-lg p-0">
  <div class="flex items-center justify-between px-6 py-4">
    <gui-card-title classname="flex items-center text-lg font-medium">
      <gui-package-plus-icon
        size="18"
        classname="mr-2"
      >
      </gui-package-plus-icon>
