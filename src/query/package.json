{
  "name": "@vltpkg/query",
  "description": "Query syntax parser that retrieves items from a graph",
  "version": "0.0.0-0.1730239248325",
  "tshy": {
    "selfLink": false,
    "dialects": [
      "esm"
    ],
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/index.ts"
    }
  },
  "dependencies": {
    "@vltpkg/dep-id": "workspace:*",
    "@vltpkg/error-cause": "workspace:*",
    "@vltpkg/graph": "workspace:*",
    "@vltpkg/types": "workspace:*",
    "postcss-selector-parser": "^6.1.2"
  },
  "devDependencies": {
    "@eslint/js": "catalog:",
    "@types/eslint__js": "catalog:",
    "@types/node": "catalog:",
    "@vltpkg/spec": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tap": "catalog:",
    "tshy": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:"
  },
  "license": "BSD-2-Clause-Patent",
  "engines": {
    "node": "20 || >=22"
  },
  "scripts": {
    "format": "prettier --write . --log-level warn --ignore-path ../../.prettierignore --cache",
    "format:check": "prettier --check . --ignore-path ../../.prettierignore --cache",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "prepare": "tshy",
    "presnap": "tshy",
    "snap": "tap",
    "pretest": "tshy",
    "test": "tap"
  },
  "tap": {
    "extends": "../../tap-config.yaml"
  },
  "prettier": "../../.prettierrc.js",
  "module": "./dist/esm/index.js",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    }
  },
  "files": [
    "dist"
  ]
}
