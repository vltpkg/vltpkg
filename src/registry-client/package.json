{
  "name": "@vltpkg/registry-client",
  "description": "Fetch package artifacts and metadata from registries",
  "version": "1.0.0-rc.11",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vltpkg/vltpkg.git",
    "directory": "src/registry-client"
  },
  "author": "vlt technology inc. <support@vlt.sh> (http://vlt.sh)",
  "dependencies": {
    "@vltpkg/cache": "workspace:*",
    "@vltpkg/cache-unzip": "workspace:*",
    "@vltpkg/error-cause": "workspace:*",
    "@vltpkg/keychain": "workspace:*",
    "@vltpkg/output": "workspace:*",
    "@vltpkg/types": "workspace:*",
    "@vltpkg/url-open": "workspace:*",
    "@vltpkg/xdg": "workspace:*",
    "cache-control-parser": "^2.0.6",
    "package-json-from-dist": "catalog:",
    "undici": "^7.16.0"
  },
  "devDependencies": {
    "@eslint/js": "catalog:",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tap": "catalog:",
    "typedoc": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:"
  },
  "license": "BSD-2-Clause-Patent",
  "engines": {
    "node": ">=22.9.0"
  },
  "scripts": {
    "format": "prettier --write . --log-level warn --ignore-path ../../.prettierignore --cache",
    "format:check": "prettier --check . --ignore-path ../../.prettierignore --cache",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "prepack": "tsc -p tsconfig.publish.json && ../../scripts/update-dist-exports.ts",
    "snap": "tap",
    "test": "tap",
    "posttest": "tsc --noEmit",
    "typecheck": "tsc --noEmit"
  },
  "tap": {
    "extends": "../../tap-config.yaml"
  },
  "prettier": "../../.prettierrc.js",
  "module": "./src/index.ts",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "default": "./src/index.ts"
      }
    },
    "./cache-entry": {
      "import": {
        "default": "./src/cache-entry.ts"
      }
    }
  },
  "files": [
    "dist"
  ]
}
