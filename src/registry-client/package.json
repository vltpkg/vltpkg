{
  "name": "@vltpkg/registry-client",
  "description": "Fetch package artifacts and metadata from registries",
  "version": "0.0.0-0",
  "tshy": {
    "selfLink": false,
    "dialects": [
      "esm"
    ],
    "exports": {
      ".": "./src/index.ts",
      "./cache-entry": "./src/cache-entry.ts",
      "./cache-interceptor": "./src/cache-interceptor.ts",
      "./package.json": "./package.json"
    }
  },
  "dependencies": {
    "@vltpkg/cache": "workspace:*",
    "@vltpkg/cache-unzip": "workspace:*",
    "@vltpkg/error-cause": "workspace:*",
    "@vltpkg/types": "workspace:*",
    "@vltpkg/xdg": "workspace:*",
    "cache-control-parser": "^2.0.5",
    "package-json-from-dist": "^1.0.0",
    "undici": "^6.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.14.8",
    "eslint": "^9.8.0",
    "prettier": "^3.3.2",
    "tap": "^20.0.1",
    "tshy": "^1.16.1",
    "typedoc": "^0.26.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.1"
  },
  "scripts": {
    "preversion": "npm test",
    "postversion": "npm publish",
    "prepublishOnly": "git push origin --follow-tags",
    "prepare": "tshy",
    "pretest": "npm run prepare",
    "presnap": "npm run prepare",
    "test": "tap",
    "snap": "tap",
    "format": "prettier --write . --log-level warn --ignore-path ../../.prettierignore --cache",
    "typedoc": "typedoc",
    "lint:format": "prettier --check . --ignore-path ../../.prettierignore --cache",
    "lint": "npm run lint:format; npm run lint:code",
    "lint:code": "eslint ."
  },
  "tap": {
    "extends": "../../tap-config.yaml"
  },
  "prettier": "../../.prettierrc.json",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "source": "./src/index.ts",
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./cache-entry": {
      "import": {
        "source": "./src/cache-entry.ts",
        "types": "./dist/esm/cache-entry.d.ts",
        "default": "./dist/esm/cache-entry.js"
      }
    },
    "./cache-interceptor": {
      "import": {
        "source": "./src/cache-interceptor.ts",
        "types": "./dist/esm/cache-interceptor.d.ts",
        "default": "./dist/esm/cache-interceptor.js"
      }
    },
    "./package.json": "./package.json"
  }
}
