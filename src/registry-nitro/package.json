{
  "name": "@vltpkg/vsr-nitro",
  "version": "1.0.0-rc.13",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vltpkg/vltpkg.git",
    "directory": "src/registry-nitro"
  },
  "author": "vlt technology inc. <support@vlt.sh> (http://vlt.sh)",
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "@neondatabase/serverless": "^1.0.2",
    "aws4fetch": "^1.0.20",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "ms": "4.0.0-nightly.202508271359",
    "nitro": "3.0.1-alpha.1",
    "rolldown": "1.0.0-beta.51",
    "unstorage": "2.0.0-alpha.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251209.0",
    "@eslint/js": "catalog:",
    "@neon-rs/load": "^0.1.82",
    "@types/node": "catalog:",
    "drizzle-kit": "^0.31.8",
    "eslint": "catalog:",
    "if-env": "^1.0.4",
    "prettier": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:",
    "wrangler": "^4.53.0"
  },
  "license": "FSL-1.1-MIT",
  "engines": {
    "node": ">=22.12.0"
  },
  "scripts": {
    "build": "nitro build",
    "postbuild": "node dev/find-libsql.mjs",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio --port 4985",
    "dev": "nitro dev",
    "format": "prettier --write . --log-level warn --ignore-path ../../.prettierignore --cache",
    "format:check": "prettier --check . --ignore-path ../../.prettierignore --cache",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "prepare": "if-env OS=Windows_NT || nitro prepare",
    "serve": "node .output/server/index.mjs",
    "serve:https": "NITRO_SSL_CERT=$(cat ./dev/localhost.pem) NITRO_SSL_KEY=$(cat ./dev/localhost-key.pem) node .output/server/index.mjs",
    "test": "exit 0",
    "posttest": "tsc --noEmit",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "../../.prettierrc.js",
  "type": "module"
}
