{
  "name": "@vltpkg/vsr-nitro",
  "private": true,
  "scripts": {
    "build": "nitro build",
    "postbuild": "node dev/find-libsql.mjs",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio --port 4985",
    "dev": "nitro dev",
    "dev:https": "concurrently \"pnpm dev\" \"node --no-warnings ./dev/https-proxy.mjs\"",
    "serve": "node .output/server/index.mjs",
    "serve:https": "concurrently \"pnpm serve\" \"node --no-warnings ./dev/https-proxy.mjs\""
  },
  "dependencies": {
    "libsql": "^0.5.20",
    "@libsql/client": "^0.15.14",
    "db0": "^0.3.2",
    "drizzle-orm": "^0.44.5",
    "h3": "^2.0.0-beta.4",
    "nitro": "npm:nitro-nightly@latest",
    "unstorage": "^1.17.1",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@neon-rs/load": "^0.0.4",
    "concurrently": "^9.2.1",
    "drizzle-kit": "^0.31.4",
    "http-proxy": "^1.18.1"
  },
  "type": "module"
}
