{
  "name": "@vltpkg/semver",
  "description": "The semantic version parser used by vlt",
  "version": "0.0.0-0",
  "tshy": {
    "selfLink": false,
    "dialects": [
      "esm"
    ],
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/index.ts"
    }
  },
  "dependencies": {
    "@vltpkg/error-cause": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.14.8",
    "@types/semver": "^7.5.8",
    "prettier": "^3.3.2",
    "semver": "^7.6.2",
    "tap": "^20.0.1",
    "tshy": "^1.16.1",
    "typedoc": "^0.26.2"
  },
  "scripts": {
    "bun": "hyperfine --warmup 3 'bun ./benchmarks/bun.js' 'bun ./benchmarks/npm.js' 'bun ./benchmarks/this.js'",
    "node": "hyperfine --warmup 3 'node ./benchmarks/npm.js' 'node ./benchmarks/this.js'",
    "benchmark": "./test/fixtures/vs-node-semver.ts",
    "preversion": "npm test",
    "postversion": "npm publish",
    "prepublishOnly": "git push origin --follow-tags",
    "prepare": "tshy",
    "pretest": "npm run prepare",
    "presnap": "npm run prepare",
    "test": "tap",
    "snap": "tap",
    "format": "prettier --write . --log-level warn --ignore-path ../../.prettierignore --cache",
    "typedoc": "typedoc",
    "lint:format": "prettier --check . --ignore-path ../../.prettierignore --cache",
    "lint": "npm run lint:format; npm run lint:code",
    "lint:code": "echo todo"
  },
  "tap": {
    "extends": "../../tap-config.yaml"
  },
  "prettier": "../../.prettierrc.json",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "source": "./src/index.ts",
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    }
  }
}
