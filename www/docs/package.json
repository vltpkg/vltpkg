    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.3",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vltpkg/cli": "workspace:*",
    "astro": "^4.16.17",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.453.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.14",
    "tailwindcss-animate": "^1.0.7",
    "typedoc": "^0.27.4",
    "typedoc-plugin-frontmatter": "^1.1.0",
    "typedoc-plugin-markdown": "^4.3.2",
    "typedoc-plugin-remark": "^1.2.0",
    "vite": "^5.4.11"
  },
  "devDependencies": {
    "@eslint/js": "catalog:",
    "@types/eslint__js": "catalog:",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "import-meta-resolve": "^4.1.0",
    "prettier": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:",
    "unified-prettier": "^2.0.1",
    "vitest": "catalog:"
  },
  "license": "BSD-2-Clause-Patent",
  "engines": {
    "node": "20 || >=22"
  },
  "scripts": {
    "astro": "astro",
    "build": "astro check && astro build",
    "dev": "astro dev",
    "dev:rebuild": "VLT_DOCS_REBUILD=true astro dev",
    "format": "prettier --write . --log-level warn --ignore-path ../../.prettierignore --cache",
    "format:check": "prettier --check . --ignore-path ../../.prettierignore --cache",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "prepare": "astro sync",
    "snap": "vitest --no-watch -u",
    "start": "astro dev",
    "test": "vitest",
    "typedoc:check": "typedoc --validation.notExported --validation.invalidLink --treatValidationWarningsAsErrors",
    "preview": "astro preview"
  },
  "prettier": "../../.prettierrc.js",
  "type": "module"
}
