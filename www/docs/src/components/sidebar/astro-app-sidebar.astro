---
import AppSidebar from './app-sidebar'
import AppSidebarSublist from './app-sidebar-sublist.tsx'
import type { Props } from '@astrojs/starlight/props'
---

<AppSidebar {...Astro.props}>
  <AppSidebar.Desktop client:only="react">
    <AppSidebarSublist
      client:only="react"
      sidebar={Astro.props.sidebar}
    />
  </AppSidebar.Desktop>
</AppSidebar>
